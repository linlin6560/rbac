<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rbac.project.mapper.SysPostMapper">

    <select id="queryPostVoList" resultType="com.rbac.project.entity.vo.QueryPostVo">
        SELECT
            id, post_name
        FROM
            sys_post
        WHERE
            post_status = 1
    </select>

</mapper>